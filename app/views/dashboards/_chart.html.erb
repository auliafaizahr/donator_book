<div class="card card-info">
  <div class="card-header">
    <h3 class="card-title">Diagram Donasi</h3>

    <div class="card-tools">
      <button type="button" class="btn btn-tool" data-card-widget="collapse">
        <i class="fas fa-minus"></i>
      </button>
      <button type="button" class="btn btn-tool" data-card-widget="remove">
        <i class="fas fa-times"></i>
      </button>
    </div>
  </div>
  <div class="card-body"><div class="chartjs-size-monitor"><div class="chartjs-size-monitor-expand"><div class=""></div></div><div class="chartjs-size-monitor-shrink"><div class=""></div></div></div>
    <canvas id="chartDonate" style="min-height: 250px; height: 250px; max-height: 250px; max-width: 100%; display: block; width: 409px;" width="409" height="250" class="chartjs-render-monitor" data-labels="<%= @category.keys %>" data-data="<%= @category.values %>"></canvas>
  </div>
  <!-- /.card-body -->
</div>