body {
  font-family: 'Roboto Slab';
  padding: 10px;
  background: linear-gradient(to right top, #e1ebf2, #d5ebf6, #c8ecf7, #b9ecf6, #abedf2, #a3eaec, #9be6e5, #94e3dd, #8fdbd6, #8ad3ce, #86ccc7, #81c4c0);
  display: block;
  margin: 0 auto;
  height: 100%;
  width: 100%;

  
  p {
    line-height: 20px;
  }
}

.wrapper-all {
  background: linear-gradient(to bottom, #f0f2f4, #daebf4, #bfe6ee, #a6e0e0, #93daca);
  background-repeat: no-repeat;
  display: block;
  margin: 0 auto;
  height: 100%;
  width: 100%;
}

.float-right {
  float: right;
}

.logo-header {
  width: 120px;
  height: auto;
}

